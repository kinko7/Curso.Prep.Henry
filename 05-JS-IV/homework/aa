
nombre= "carlos";
apellido="mar;"
class Persona { 
constructor (nombre,apellido) { 
this.nombre=nombre;
this.apellido=apellido;
}

saludar(){return "hola ""
+ this.nombre}
getnombre(){return this.nombre}




class Empleado extends Persona{
	constructor (nombre,apellido,empleado,sueldo)
super (nombre,apellido);
this.empleado=empleado;
this.sueldo=sueldo;
}
}

var rene= new Empleado("rene","marletta","developer",500);

