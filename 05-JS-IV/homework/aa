var max = numeros[0];
    var min = numeros[0];
function menorMayor(numeros) {

var max = numeros[0];
    var min = numeros[0];
 for (var i = 1; i < numeros.length; i++) {
        if (numeros[i] < min) min = numeros[i]
    } {
        if (numeros[i] > max) max = numeros[i]
    } {
        return [min[], max[]];
    }
}